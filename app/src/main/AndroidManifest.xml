<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.LCSDNL.controledeproducao">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ControleDeProducao">
        <activity android:name=".cadastroActivity.Cadastro_Modelo_Activity" />
        <activity
            android:name=".rep_modelos.Lista_Modelos_Activity"
            android:label="@string/title_activity__lista__modelos"
            android:theme="@style/Theme.ControleDeProducao.NoActionBar" />
        <activity
            android:name=".lista_funcionario.Lista_Funcionarios_Activity"
            android:label="@string/title_activity__lista__funcionarios"
            android:theme="@style/Theme.ControleDeProducao.NoActionBar" />
        <activity android:name=".cadastroActivity.Cadastro_Funcionario_Activity" />
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ControleDeProducao.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>